{
  "name": "@postenbring/hedwig-tokens",
  "version": "0.0.8",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "tokens-output/**",
    "tailwind.config.cjs",
    "tailwind.config.d.cts"
  ],
  "scripts": {
    "build": "node --import tsx build.ts",
    "dev": "node --import tsx --watch-preserve-output --watch-path lib --watch-path tokens-source --watch-path build.ts --watch build.ts",
    "lint": "eslint \"*.ts*\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "devDependencies": {
    "@types/node": "20.10.7",
    "color2k": "2.0.3",
    "eslint": "^8.56.0",
    "eslint-config-custom": "*",
    "hedwig-tsconfig": "workspace:*",
    "style-dictionary": "3.9.1",
    "style-dictionary-utils": "2.0.7",
    "tailwindcss": "3.4.1",
    "tsx": "4.7.0",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
