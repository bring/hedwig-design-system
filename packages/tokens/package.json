{
  "name": "@postenbring/hedwig-tokens",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "assets/**",
    "dist/**"
  ],
  "scripts": {
    "build": "tsx build.ts && ./postprocess.sh",
    "dev": "npm run build && chokidar \"build.ts\" \"tokens/**\" -c \"npm run build\"",
