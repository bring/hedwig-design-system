{
  "name": "@postenbring/hedwig-tokens",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "assets/**",
    "dist/**"
  ],
  "scripts": {
    "build": "node --import tsx build.ts",
    "dev": "node --import tsx --watch-path tokens-source --watch-path build.ts --watch build.ts",
