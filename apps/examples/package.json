{
  "name": "examples",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "wait-on ./node_modules/@postenbring/hedwig-css/dist/index.css ./node_modules/@postenbring/hedwig-react/dist/index.js && remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo node_modules build"
  },
  "dependencies": {
    "@postenbring/hedwig-css": "workspace:*",
    "@postenbring/hedwig-react": "workspace:*",
    "@postenbring/hedwig-tokens": "workspace:*",
    "@radix-ui/react-slot": "1.1.0",
    "@remix-run/node": "^2.11.1",
    "@remix-run/react": "^2.11.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shiki": "1.12.1"
  },
  "devDependencies": {
    "@mdx-js/rollup": "3.0.1",
    "@remix-run/dev": "^2.11.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^7.16.1",
    "autoprefixer": "10.4.20",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "8.4.41",
    "remark-frontmatter": "5.0.0",
    "remark-mdx-frontmatter": "5.0.0",
    "tailwindcss": "3.4.9",
    "typescript": "^5.5.4",
    "vite": "^5.2.13",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "version": "0.0.0"
}
